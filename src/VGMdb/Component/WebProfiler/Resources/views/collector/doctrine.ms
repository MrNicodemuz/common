{{#panel}}
<div id="collector-content">
{{#collector.data}}
<h2>Doctrine Queries</h2>
{{#queries}}
<table>
  <thead>
    <tr>
      <th>Connection: {{connection}} - Time: {{time}} sec</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>
        {{{sql_pretty}}}
      </td>
    </tr>
  </tbody>
</table>
{{/queries}}
{{^queries}}
<p><em>No queries.</em></p>
{{/queries}}
{{/collector.data}}
</div>
{{/panel}}
{{#toolbar}}
<span class="label">
  <span class="icon"><img src="data:image/png;base64," alt="Doctrine" /></span>
  <strong>Doctrine</strong>
</span>
{{/toolbar}}